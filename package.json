{
  "name": "mwoffliner",
  "version": "1.11.12",
  "description": "Mediawiki ZIM scraper",
  "main": "lib/mwoffliner.lib.js",
  "license": "GPL-3.0",
  "author": {
    "name": "Emmanuel Engelhart",
    "email": "kelson@kiwix.org",
    "url": "https://kiwix.org/"
  },
  "scripts": {
    "example-wiki": "docker run -p 8080:80 -v $(pwd)/test/mediawiki:/var/www/data -it openzim/mediawiki",
    "mwoffliner": "node ./lib/cli.js",
    "tape": "tape -r tsconfig-paths/register -r ts-node/register",
    "test:e2e": "npm run tape -- test/e2e/*.test.ts | tap-spec",
    "test:unit": "npm run tape -- test/unit/*.test.ts | tap-spec",
    "test": "npm run test:unit && npm run test:e2e",
    "jest": "jest",
    "codecov": "nyc --reporter=lcov npm t",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json -c tslint.json",
    "build": "./dev/build.sh",
    "watch": "./dev/watch.sh",
    "prepublish": "npm run build",
    "redis:start": "docker run -p 6379:6379 --name redis-mwoffliner -d redis",
    "redis:kill": "docker rm -f redis-mwoffliner || :",
    "redis": "npm run redis:kill && npm run redis:start",
    "start": "./node_modules/.bin/ts-node ./src/cli.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/**/*",
      "node_modules"
    ]
  },
  "bin": {
    "mwoffliner": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openzim/mwoffliner.git"
  },
  "bugs": {
    "url": "https://github.com/openzim/mwoffliner/issues"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "engine-strict": {
    "node": ">=8.12.0"
  },
  "dependencies": {
    "@ladjs/country-language": "^1.0.0",
    "@openzim/libzim": "2.4.4",
    "@openzim/libzim": "github:openzim/node-libzim#move_to_libzim7",
    "@types/async": "^3.2.15",
    "@types/backoff": "^2.5.2",
    "@types/bluebird": "^3.5.36",
    "@types/file-type": "^10.9.1",
    "@types/html-minifier": "^4.0.2",
    "@types/imagemin": "^7.0.1",
    "@types/imagemin-gifsicle": "^7.0.1",
    "@types/imagemin-jpegtran": "^5.0.1",
    "@types/imagemin-optipng": "^5.2.1",
    "@types/imagemin-webp": "^5.1.2",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.0",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.10",
    "@types/sharp": "^0.30.4",
    "@types/yargs": "^15.0.14",
    "async": "^3.2.4",
    "aws-sdk": "^2.1189.0",
    "axios": "^0.27.2",
    "backoff": "^2.5.0",
    "deepmerge": "^4.2.2",
    "details-element-polyfill": "^2.4.0",
    "domino": "^2.1.6",
    "execa": "^4.1.0",
    "follow-redirects": "^1.15.1",
    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.1",
    "imagemin-advpng": "^6.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegoptim": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^6.0.0",
    "md5": "^2.3.0",
    "merge": "^2.1.1",
    "mkdirp": "^1.0.4",
    "mocha": "^8.4.0",
    "p-map": "^4.0.0",
    "parsoid": "^0.11.1",
    "public-ip": "^4.0.4",
    "redis": "^3.1.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "service-mobileapp-node": "git+https://github.com/wikimedia/mobileapps.git#d244439",
    "service-runner": "^2.9.0",
    "sharp": "^0.30.7",
